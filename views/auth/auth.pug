extends ../layout

block content

    div.container
        div.jumbotron.text-center
            h1
                span.fa.fa-lock
                = ' ' + title
            p Login or Register with:
            a.btn.btn-default(href = '/auth/login')
                span.fa.fa-user
                |  Local Login
            a.btn.btn-default(href = '/auth/signup')
                span.fa.fa-user
                |  Local Signup